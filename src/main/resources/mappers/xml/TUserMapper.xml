        <result column="Issue_forum" property="issueForum" />
        <result column="Collect_re" property="collectRe" />
        <result column="Collect_forum" property="collectForum" />
        <result column="Read_re" property="readRe" />
        <result column="Read_forum" property="readForum" />
        <result column="In_date" property="inDate" />
        <result column="Histroy_date" property="histroyDate" />
        <result column="Comment" property="Comment" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        UUID, Status, Username, password, Avatr_URL, Issue_re, Issue_forum, Collect_re, Collect_forum, Read_re, Read_forum, In_date, Histroy_date, Comment
    </sql>

</mapper>
